{
    "name": "leearvin/hyperf-tinker",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "tinker",
        "swoole"
    ],
    "authors": [
        {
            "name": "Arvin Hermit",
            "email": "arvin.hermit@gmail.com",
            "homepage": "https://github.com/Arvin-Lee"
        }
    ],
    "description": "Powerful REPL for the Hyperf framework.",
    "autoload": {
        "psr-4": {
            "Hyperf\\Tinker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Tinker\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-swoole": ">=4.5",
        "hyperf/di": "~2.2.0",
        "psy/psysh": "^0.10.4",
        "hyperf/command": "~2.2.0",
        "hyperf/contract": "~2.2.0"
    },
    "require-dev": {
        "hyperf/testing": "2.2.*"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Hyperf\\Tinker\\ConfigProvider"
        }
    }
}
